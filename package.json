{
  "name": "chronos",
  "version": "1.0.0",
  "description": "Tool to pull Wikipedia event data; Node server to serve the data",
  "type": "commonjs",
  "files": [
    "bin"
  ],
  "bin": {
    "chronos": "./bin/index.js"
  },
  "lint-staged": {
    "{,tools}/**/*.{js,ts}": [
      "npm run pretty",
      "npm run lint"
    ]
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "echo 'Tests not set up!'",
    "pretty": "prettier --write \"{,!(node_modules|bin|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint \"{,!(node_modules|bin)/**/}*.{js,ts}\" --fix",
    "prepare": "husky install"
  },
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.2",
    "@types/figlet": "^1.5.5",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "commander": "^9.4.1",
